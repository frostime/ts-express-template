{
  "compilerOptions": {
      "noImplicitAny": false,
      "noEmitOnError": true,
      "removeComments": false,
      "target": "ES2021",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "module": "es6",
      "typeRoots": [
          "./types",
          "./node_modules/@types",
      ]
  },
  "exclude": [
      "node_modules"
  ],
  "include": [
      "./src/"
  ]
}
